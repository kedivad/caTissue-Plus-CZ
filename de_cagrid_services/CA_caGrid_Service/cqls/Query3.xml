<?xml version="1.0" encoding="UTF-8"?>
<CQLQuery xmlns="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
	<Target name="clinical_annotation.AlcoholHealthAnnotation">
		<Group logicRelation="AND">
			<Attribute name="drinksPerWeek" value="3" predicate="GREATER_THAN" />
			<Attribute name="agent" value="Ethanol" predicate="EQUAL_TO" />
		</Group>

	</Target>
</CQLQuery>
