<?xml version="1.0" encoding="UTF-8"?>
<CQLQuery xmlns="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
	<Target name="clinical_annotation.SmokingHealthAnnotation">
		<Group logicRelation="AND">
			<Attribute name="packsPerDay" value="2" predicate="GREATER_THAN" />
			<Attribute name="agent" value="Tobacco" predicate="EQUAL_TO" />
		</Group>
	</Target>
</CQLQuery>
