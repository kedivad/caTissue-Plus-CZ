<?xml version="1.0" encoding="UTF-8"?>
<CQLQuery xmlns="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
	<Target name="clinical_annotation.TreatmentAnnotation">
		<Group logicRelation="AND">
			<Attribute name="agent" value="ANASTROZOLE" predicate="EQUAL_TO" />
			<Association roleName="Duration" name="clinical_annotation.Duration">
				<Group logicRelation="AND">
					<Attribute name="durationInDays" value="20" predicate="EQUAL_TO" />
				</Group>
			</Association>
		</Group>
	</Target>
</CQLQuery>
