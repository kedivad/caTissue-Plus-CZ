<ScrubberConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ScrubberConfiguration.xsd">
	<ScrubberImplementation>XML_TEXT</ScrubberImplementation>
	<TextProcessingRules>
		<SavedIdentifiers>
			<Label>Names</Label>
			<FileLocation>@@CONTEXT_FOLDER@@/names.list</FileLocation>
			<ReplaceWith>xxxNAMExxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<RegularExpressions>
			<FileLocation>@@CONTEXT_FOLDER@@/regex.list</FileLocation>
			<ReplaceWith>xxxx</ReplaceWith>
			<PrintRegexNameInResult>true</PrintRegexNameInResult>
		</RegularExpressions>
	</TextProcessingRules>
	<XMLTextProcessingRules>
		<Validation>
			<EnableValidation>true</EnableValidation>
			<SchemaLocation/>
		</Validation>
		<IdentifyingRegions>
			<Region>
				<Label>FirstName</Label>
				<XPATH>/Envelope/Header/FirstName</XPATH>
				<ReplaceWith>xxxFirstNamexxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>LastName</Label>
				<XPATH>/Envelope/Header/LastName</XPATH>
				<ReplaceWith>xxxLast Namexxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>DateOfBirth</Label>
				<XPATH>/Envelope/Header/DateOfBirth</XPATH>
				<ReplaceWith>xxxDateOfBirthxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>AccessionNumber</Label>
				<XPATH>/Envelope/Header/AccessionNumber</XPATH>
				<ReplaceWith>xxxAccessionNumberxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>LocalMRN</Label>
				<XPATH>/Envelope/Header/LocalMRN</XPATH>
				<ReplaceWith>xxxLocalMRNxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>Source</Label>
				<XPATH>/Envelope/Header/Source</XPATH>
				<ReplaceWith>xxxSourcexxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
		</IdentifyingRegions>
		<RegionsToScrub>
			<Region>
				<Label>ClinicalInformation</Label>
				<XPATH>/Envelope/Body/PathologyCase/Clinical/ClinicalInformation</XPATH>
			</Region>
			<Region>
				<Label>GrossDescription</Label>
				<XPATH>/Envelope/Body/PathologyCase/GrossDescriptionText</XPATH>
			</Region>
			<Region>
				<Label>DiagnosisText</Label>
				<XPATH>/Envelope/Body/PathologyCase/DiagnosisText</XPATH>
			</Region>
			<Region>
				<Label>FullReportText</Label>
				<XPATH>/Envelope/Body/PathologyCase/FullReportText</XPATH>
			</Region>
		</RegionsToScrub>
	</XMLTextProcessingRules>
	<Recorder>
		<Implementation>DEFAULT</Implementation>
		<EnableRecording>true</EnableRecording>
		<ConfigParam key="Text" value="Text"/>
	</Recorder>
</ScrubberConfiguration>
